-        }
-      }
-      done()
-    })
-  }, 240000)
-})
-
-function iterate(obj, stack, array) {
-  for (const property in obj) {
-    if (obj.hasOwnProperty(property)) {
-      if (typeof obj[property] === "object") {
-        iterate(obj[property], `${stack}.${property}`, array)
-      } else {
-        array.push(`${stack.slice(1)}.${property}`)
